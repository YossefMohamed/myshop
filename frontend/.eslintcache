]);\r\n\r\n  const addToCartHandler = () => {\r\n    props.history.push(`/cart/${props.match.params.id}?qty=${qty}`);\r\n  };\r\n  const { loading, product, error } = props.product;\r\n\r\n  return (\r\n    <>\r\n      {loading ? (\r\n        <Loader />\r\n      ) : props.product.error ? (\r\n        <Massage variant=\"danger\">{error}</Massage>\r\n      ) : (\r\n        <>\r\n          <Link className=\" btn btn-dark mb-4\" to=\"/\">\r\n            Go Back\r\n          </Link>\r\n          <Row className=\"mb-4\">\r\n            <Col lg={6}>\r\n              <Image src={product.image} alt={product.name} fluid />\r\n            </Col>\r\n\r\n            <Col lg={3} className=\"my-1\">\r\n              <ListGroup variant=\"flush\">\r\n                <ListGroup.Item>\r\n                  <h3>{product.name}</h3>\r\n                </ListGroup.Item>\r\n                <ListGroupItem>\r\n                  <Rating\r\n                    value={product.rating}\r\n                    number={`${product.numberReviews} reviews`}\r\n                  />\r\n                </ListGroupItem>\r\n                <ListGroupItem>Price : ${product.price}</ListGroupItem>\r\n                <ListGroupItem>\r\n                  Description : {product.description}\r\n                </ListGroupItem>\r\n              </ListGroup>\r\n            </Col>\r\n\r\n            <Col lg={3} className=\"align-middle\">\r\n              <Card>\r\n                <ListGroup variant=\"flush\">\r\n                  <ListGroupItem>\r\n                    <Row>\r\n                      <Col>Price : </Col>\r\n                      <Col>\r\n                        <strong>${product.price}</strong>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroupItem>\r\n                </ListGroup>\r\n                <ListGroup variant=\"flush\">\r\n                  <ListGroupItem>\r\n                    <Row>\r\n                      <Col>Status : </Col>\r\n                      <Col>\r\n                        <strong>\r\n                          {product.countInStock > 0\r\n                            ? \"In Stock\"\r\n                            : \"Out Of Stock\"}\r\n                        </strong>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroupItem>\r\n                  {product.countInStock > 0 && (\r\n                    <ListGroupItem>\r\n                      <Row>\r\n                        <Col className=\"d-flex align-items-center\">Qty:</Col>\r\n                        <Col>\r\n                          <Form.Control\r\n                            as=\"select\"\r\n                            onChange={(e) => setQtu(e.target.value)}\r\n                            size=\"sm\"\r\n                          >\r\n                            {[...Array(product.countInStock).keys()].map(\r\n                              (x) => (\r\n                                <option key={x + 1} value={x + 1}>\r\n                                  {x + 1}\r\n                                </option>\r\n                              )\r\n                            )}\r\n                          </Form.Control>\r\n                        </Col>\r\n                      </Row>\r\n                    </ListGroupItem>\r\n                  )}\r\n                  <ListGroupItem>\r\n                    <Button\r\n                      className=\"btn-block\"\r\n                      onClick={addToCartHandler}\r\n                      type=\"button\"\r\n                      disabled={product.countInStock === 0}\r\n                    >\r\n                      ADD TO CART\r\n                    </Button>\r\n                  </ListGroupItem>\r\n                </ListGroup>\r\n              </Card>\r\n            </Col>\r\n          </Row>\r\n        </>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n  return { product: state.productDetails };\r\n};\r\nexport default connect(mapStateToProps)(ProductScreen);\r\n","Y:\\yossef-shop\\frontend\\src\\store.js",[],"Y:\\yossef-shop\\frontend\\src\\reducers\\productReducers.js",[],"Y:\\yossef-shop\\frontend\\src\\actions\\productAction.js",[],"Y:\\yossef-shop\\frontend\\src\\loader\\loader.jsx",[],"Y:\\yossef-shop\\frontend\\src\\message\\message.jsx",[],"Y:\\yossef-shop\\frontend\\src\\products.js",[],"Y:\\yossef-shop\\frontend\\src\\cartScreen\\cartScreen.jsx",["94"],"import React from \"react\";\r\nimport Massage from \"./../message/message\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport {\r\n  Row,\r\n  Col,\r\n  ListGroup,\r\n  Image,\r\n  Form,\r\n  Button,\r\n  Card,\r\n  ListGroupItem,\r\n} from \"react-bootstrap\";\r\nimport { addToCart, removeFromCart } from \"../actions/cartAction\";\r\n\r\nfunction CartScreen(props) {\r\n  const productId = props.match.params.id;\r\n  const qty = props.location.search\r\n    ? Number(props.location.search.split(\"=\")[1])\r\n    : 1;\r\n  console.log(qty);\r\n  const dispatch = useDispatch();\r\n  const { cartItems } = useSelector((state) => state.cart);\r\n  const removeFromCartHandler = (id) => {\r\n    dispatch(removeFromCart(id));\r\n  };\r\n  const checkoutHandler = () => {\r\n    props.history.push(\"/signin?redirect=shipping\");\r\n  };\r\n  React.useEffect(() => {\r\n    if (productId) {\r\n      dispatch(addToCart(productId, qty));\r\n    }\r\n  }, [dispatch, productId, qty]);\r\n  return (\r\n    <Row>\r\n      <Col md={8}>\r\n        <h1>Shopping Cart</h1>\r\n        {cartItems.length === 0 ? (\r\n          <Massage>\r\n            YourCart Is Empty<Link to=\"/\">Go Back</Link>\r\n          </Massage>\r\n        ) : (\r\n          <ListGroup variant=\"flush\">\r\n            {cartItems.map((item) => (\r\n              <ListGroup.Item key={item.product}>\r\n                <Row>\r\n                  <Col md={2}>\r\n                    <Image src={item.image} alt={item.name} fluid />\r\n                  </Col>\r\n                  <Col md={2}>\r\n                    <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                  </Col>\r\n                  <Col md={2}>${item.price}</Col>\r\n                  <Col md={2}>\r\n                    <Form.Control\r\n                      as=\"select\"\r\n                      onChange={(e) =>\r\n                        dispatch(\r\n                          addToCart(item.product, Number(e.target.value))\r\n                        )\r\n                      }\r\n                      value={item.qty}\r\n                      size=\"sm\"\r\n                    >\r\n                      {[...Array(item.countInStock).keys()].map((x) => (\r\n                        <option key={x + 1} value={x + 1}>\r\n                          {x + 1}\r\n                        </option>\r\n                      ))}\r\n                    </Form.Control>\r\n                  </Col>\r\n                  <Col md={2}>\r\n                    <Button\r\n                      type=\"button\"\r\n                      variant=\"light\"\r\n                      onClick={() => removeFromCartHandler(item.product)}\r\n                    >\r\n                      <i className=\"fas fa-trash\"></i>\r\n                    </Button>\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n            ))}\r\n          </ListGroup>\r\n        )}\r\n      </Col>\r\n      <Col md={4}>\r\n        <ListGroup>\r\n          <ListGroupItem>\r\n            <h2>\r\n              SubTotal ({cartItems.reduce((acc, cur) => acc + cur.qty, 0)})\r\n              items\r\n            </h2>\r\n            $\r\n            {cartItems\r\n              .reduce((acc, item) => acc + item.qty * item.price, 0)\r\n              .toFixed(2)}\r\n          </ListGroupItem>\r\n          <ListGroupItem>\r\n            <Button\r\n              type=\"button\"\r\n              className=\"btn-block\"\r\n              disabled={cartItems.length === 0}\r\n              onClick={checkoutHandler}\r\n            >\r\n              CheckOut\r\n            </Button>\r\n          </ListGroupItem>\r\n        </ListGroup>\r\n      </Col>\r\n    </Row>\r\n  );\r\n}\r\n\r\nexport default CartScreen;\r\n","Y:\\yossef-shop\\frontend\\src\\reducers\\cartReducers.jsx",["95"],"\r\nexport default (state = { cartItems: [] }, { type, payload }) => {\r\n  switch (type) {\r\n    case \"CART_ADD_ITEM\":\r\n      const existItem = state.cartItems.find(\r\n        (x) => x.product === payload.product\r\n      );\r\n      if (existItem) {\r\n        return {\r\n          ...state,\r\n          cartItems: state.cartItems.map((x) =>\r\n            x.product === existItem.product ? payload : x\r\n          ),\r\n        };\r\n      }\r\n      return { ...state, cartItems: [...state.cartItems, payload] };\r\n    case \"CART_REMOVE_ITEM\":\r\n      return {\r\n        ...state,\r\n        cartItems: state.cartItems.filter((x) => x.product !== payload),\r\n      };\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n","Y:\\yossef-shop\\frontend\\src\\actions\\cartAction.jsx",[],"Y:\\yossef-shop\\frontend\\src\\reducers\\userReducers.js",[],"Y:\\yossef-shop\\frontend\\src\\loginScreen\\loginScreen.jsx",[],"Y:\\yossef-shop\\frontend\\src\\actions\\userAction.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":7,"column":32,"nodeType":"102","messageId":"103","endLine":7,"endColumn":38},{"ruleId":"100","severity":1,"message":"104","line":1,"column":17,"nodeType":"102","messageId":"103","endLine":1,"endColumn":25},{"ruleId":"100","severity":1,"message":"105","line":4,"column":8,"nodeType":"102","messageId":"103","endLine":4,"endColumn":16},{"ruleId":"106","severity":1,"message":"107","line":15,"column":6,"nodeType":"108","endLine":15,"endColumn":8,"suggestions":"109"},{"ruleId":"106","severity":1,"message":"110","li